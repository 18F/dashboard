---
permalink: /api/projects/
---
{
	"projects": {
		"project": [{% for project in site.data.projects %}{% if project.status != "Hold" %}
			{
				"project": "{{ project.project }}",
				"name": "{{ project.name }}",
				"github": 
				[
					{% for url in project.github %}"{{ url }}"{% if forloop.rindex0 != 0 %},{% endif %}{% endfor %}
				],
				"description": "{{ project.description }}",
				"client": "{{ project.client }}",
				"partners": "{{ project.partners }}",
				"impact": "{{ project.impact}}",
				"stage": "{{ project.stage }}",
				"milestones":"{{ project.milestones }}",
				"contact":"{{ project.contact }}",
				"stack":"{{ project.stack }}",
				"team":"{{ project.team }}",
				"license":"{{ project.license }}",
				"licenselink":"{{ project.licenselink }}",
				"links":"{{ project.links }}"
			}{% if project != site.data.projects.last %},{% endif %}{% endif %}{% endfor %}
		]
	}
}