{
  "port":                5000,
  "repoDir":             "/home/site/team-api/dashboard",
  "githubRepo":          "18F/dashboard",
  "defaultBranch":       "staging",
  "teamApiBranch":       "team-api",
  "updateScript":        "_data/import-public.rb",
  "git":                 "/usr/bin/git",
  "teamApi":             "https://team-api.18f.gov/public/api",
  "teamApiRepo":         "18F/team-api",
  "teamApiBranch":       "master",
  "commitMessage":       "Automated Team API update",
  "pullRequestTemplate": {
    "title": "Automated Team API update",
    "body":  "This is an automated update from the 18F Team API",
    "head":  "team-api-update",
    "base":  "staging"
  },
  "gitHubApiOptions":    {
    "hostname": "github.com",
    "port":     443,
    "path":     "/",
    "method":   "POST"
  }
}
